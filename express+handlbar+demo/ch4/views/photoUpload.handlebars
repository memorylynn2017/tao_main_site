 <!-- jq上传 -->
<span class="btn btn-default btn-file">
	Upload
	<input type="file" data-url='/upload' multiple class="form-control" id="fieldPhoto" accept="image/*" name="photo" require>
</span>
<div id="fileUploads"></div>
<script src='/vendor/jquery.ui.widget.js'></script>
<script src='/vendor/jquery.iframe-transport.js'></script>
<script src='/vendor/jquery.fileupload.js'></script>
<script>
	$(document).ready(function(){
		$('#fieldPhoto').fileupload({
			dataType:'json',
			done: function(e,data){

				$.each(data.result.files, function (index, file) {
					$('#fileUploads').append($('<div class="upload">'+
						'<span class="glyphicon glyphicon-ok"></span>'+
						file.originalName+'</div>'));
				});
			}
		});
	});
</script>